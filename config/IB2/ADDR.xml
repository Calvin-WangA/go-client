                <port>-1</port>
            </address>
        </addresses>
        <!-- 使用的加减密算法-->
        <encryption>
            <name>sm4</name>
            <pubKey>111</pubKey>
            <priKey>222</priKey>
            <flag>0</flag>
        </encryption>
        <!-- 使用的编码格式ss -->
        <encode>GBK</encode>
        <!-- 使用的协议 -->
